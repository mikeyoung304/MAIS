{
  "name": "@macon/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Macon AI Solutions API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "doctor": "tsx scripts/doctor.ts",
    "dev": "tsx watch src/index.ts",
    "dev:mock": "cross-env ADAPTERS_PRESET=mock tsx watch src/index.ts",
    "dev:real": "cross-env ADAPTERS_PRESET=real tsx watch src/index.ts",
    "build": "npm run prisma:generate && tsc -b",
    "start": "npx tsx src/index.ts",
    "typecheck": "tsc -b --noEmit",
    "test": "vitest run --reporter=verbose",
    "test:serial": "vitest --pool=forks --poolOptions.forks.singleFork",
    "test:watch": "vitest",
    "test:integration": "DATABASE_URL=$DATABASE_URL_TEST vitest run test/integration/ --no-coverage",
    "test:integration:watch": "DATABASE_URL=$DATABASE_URL_TEST vitest watch test/integration/",
    "coverage": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "vitest run --coverage test/ --exclude=\"test/integration/**\"",
    "test:coverage:report": "open coverage/index.html",
    "test:agent-eval": "vitest run --config vitest.config.agent-eval.ts",
    "test:agent-eval:parity": "vitest run --config vitest.config.agent-eval.ts test/agent-eval/parity/",
    "test:agent-eval:safety": "vitest run --config vitest.config.agent-eval.ts test/agent-eval/safety/",
    "test:agent-eval:outcomes": "vitest run --config vitest.config.agent-eval.ts test/agent-eval/outcomes/",
    "prisma:generate": "prisma generate && node scripts/prisma-postgenerate.js",
    "prisma:postgenerate": "node scripts/prisma-postgenerate.js",
    "db:migrate": "prisma migrate dev --name initial_schema",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:production": "cross-env SEED_MODE=production tsx prisma/seed.ts",
    "db:seed:e2e": "cross-env SEED_MODE=e2e tsx prisma/seed.ts",
    "db:seed:demo": "cross-env SEED_MODE=demo tsx prisma/seed.ts",
    "db:seed:dev": "cross-env SEED_MODE=dev tsx prisma/seed.ts",
    "db:seed:all": "cross-env SEED_MODE=all tsx prisma/seed.ts",
    "create-tenant": "tsx scripts/create-tenant.ts",
    "create-tenant-with-stripe": "tsx scripts/create-tenant-with-stripe.ts",
    "delete-test-users": "tsx scripts/delete-test-users.ts",
    "delete-test-tenants": "tsx scripts/delete-test-tenants.ts",
    "test:commission": "tsx scripts/test-commission.ts",
    "test:stripe-connect": "tsx scripts/test-stripe-connect.ts",
    "db:diagnose": "tsx scripts/diagnose-database.ts",
    "validate:migrations": "tsx scripts/validate-migrations.ts"
  },
  "dependencies": {
    "@google/adk": "^0.2.4",
    "@google/adk-devtools": "^0.2.4",
    "@google/genai": "^1.35.0",
    "@macon/contracts": "*",
    "@macon/shared": "*",
    "@paralleldrive/cuid2": "^2.3.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^10.35.0",
    "@sentry/profiling-node": "^10.35.0",
    "@supabase/supabase-js": "^2.84.0",
    "@ts-rest/express": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.2",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.10",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.66.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "file-type": "^16.5.4",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.35.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "openapi-types": "^12.1.3",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "prisma": "^7.2.0",
    "prom-client": "^15.1.3",
    "stripe": "^19.1.0",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.7.0",
    "validator": "^13.15.23",
    "xss": "^1.0.15",
    "xstate": "^5.25.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
