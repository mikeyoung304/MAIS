{
  "name": "@macon/web-next",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:turbo": "next dev --turbo --port 3000",
    "dev:fresh": "rm -rf .next .turbo node_modules/.cache && npm run dev",
    "dev:no-sw": "DISABLE_SW=true npm run dev",
    "build": "next build",
    "build:clean": "rm -rf .next .turbo && npm run build",
    "build:deps": "npm run build -w @macon/contracts && npm run build -w @macon/shared",
    "vercel-build": "cd ../.. && npm install && npm run build -w @macon/contracts && npm run build -w @macon/shared && cd apps/web && next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@macon/contracts": "*",
    "@macon/shared": "*",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@sentry/nextjs": "^10.35.0",
    "@tanstack/react-query": "^5.56.2",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "@ts-rest/core": "^3.51.0",
    "@ts-rest/react-query": "^3.51.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "immer": "^11.1.3",
    "lucide-react": "^0.441.0",
    "next": "14.2.35",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18.3.1",
    "react-day-picker": "^9.4.4",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^4.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.2",
    "vaul": "^1.1.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.56.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
